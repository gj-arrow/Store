@model IEnumerable<Store.Domain.Concrete.ApplicationUser>

@{
    ViewBag.Title = "Админ панель: список пользователей";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
<script type="text/javascript" src="../../Scripts/Script.js"></script>
<script type="text/javascript" src="../../Scripts/jquery.confirm.js"></script>
<link rel="stylesheet" type="text/css" href="../../Content/jquery_confirm.css" />

<div class="panel panel-default">
    <div class="panel-heading">
        <h3>Список пользователей</h3>
    </div>
    @Html.ValidationSummary()
    <div class="panel-body">
        <table class="table table-striped table-condensed table-bordered">
            <tr>
                <th>Name(Email)</th>
                <th class="text-center">Действия</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Email</td>
                    <td class="text-center item">
                        <div>
                            <p class="delete btn btn-default btn-xs" onclick="delUser('@item.Email')">Удалить</p>
                        </div>
                    </td>
                </tr>
            }
        </table>
    </div>
</div>